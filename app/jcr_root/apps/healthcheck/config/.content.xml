<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
	xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
	jcr:primaryType="nt:folder" >
    
	<de.joerghoh.cq5.healthcheck.impl.providers.MBeanStatusProvider-installed_bundles_no_installed
		jcr:primaryType="sling:OsgiConfig" 
		mbean.name="de.joerghoh.cq5.jmx:id\=bundles"
		mbean.property="InstalledBundles.WARN.!\=.0" 
		mbean.providerHint="no bundle in installed state" />
	<de.joerghoh.cq5.healthcheck.impl.providers.MBeanStatusProvider-installed_bundles_no_resolved
		jcr:primaryType="sling:OsgiConfig" 
		mbean.name="de.joerghoh.cq5.jmx:id\=bundles"
		mbean.property="ResolvedBundles.WARN.!\=.0" 
		mbean.providerHint="no bundle in installed state" />
		
	<de.joerghoh.cq5.healthcheck.impl.HealthStatusServiceImpl
		jcr:primaryType="sling:OsgiConfig"
		bundle.threshold="2" />
		
</jcr:root>